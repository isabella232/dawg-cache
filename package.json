{
  "name": "dawg-cache",
  "version": "0.1.0-dev1",
  "description": "Javascript/C++ DAWG implementation",
  "main": "index.js",
  "private": true,
  "gypfile": true,
  "dependencies": {
    "fast-crc32c": "^1.0.1",
    "nan": "*",
    "node-pre-gyp": "0.5.x"
  },
  "devDependencies": {
    "collections": "^1.2.4",
    "queue-async": "*",
    "request": "*",
    "tape": "4.2.2"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "tape test/*.test.js"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "binary": {
    "module_name": "jsdawg",
    "module_path": "./lib/",
    "host": "https://mapbox-node-binary.s3.amazonaws.com",
    "remote_path": "./{name}/v{version}",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz"
  }
}
